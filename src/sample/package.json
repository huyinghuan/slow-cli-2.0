{
  "name": "sample",
  "version": "1.0",
  "silky-version": "2.0.0",
  "silky-plugin": {
    "hbs": {
      "__stop": false,
      "__source": "./plugin/hbs",
      "__setting": {
        "root": "/template",
        "data-config": "hbs-data-config.js",
        "global": "hbs-global.js",
        "global-root": "__global"
      }
    },
    "sp-less": {
      "_env_global": "global.less",
      "global": [
        "css/global.less"
      ],
      "ignore": [
        "(css/module/)"
      ]
    },
    "sp-autoprefixer": {
      "html": true
    },
    "tar-send": {
      "__stop": true,
      "__source": "./plugin/tar-send"
    },
    "sp-mini": {},
    "sp-merge": [
      {
        "_regexp": true,
        "target": "/js/global.js",
        "source": [
          "(\\-componts\\.js)$",
          "B-beMerge"
        ]
      },
      {
        "suffix": "js/",
        "postfix": ".js",
        "target": "/js/global-1",
        "source": [
          "A-componts",
          "B-beMerge"
        ]
      }
    ],
    "sp-proxy": {}
  },
  "silky": {
    "port": 14422,
    "index": "index.html"
  },
  "silky-build": {
    "outdir": "./build",
    "ignore": [
      "node_modules",
      "(\\/\\.[^/]+)$",
      "plugin",
      "(\\.tar)$"
    ]
  },
  "__registry": "https://registry.npmjs.com/",
  "dependencies": {
    "sp-autoprefixer": "1.0.1",
    "sp-less": "1.0.0",
    "sp-merge": "1.0.0",
    "sp-mini": "1.0.0",
    "sp-proxy": "1.0.0"
  }
}
