{
  "name": "sample",
  "version": "1.0",
  "silky-version": "2.0.0",
  "silky-plugin": {
    "hbs": {
      "__source": "./plugin/hbs",
      "__setting": {
        "root": "/template"
      }
    },
    "less": {
      "__source": "./plugin/less"
    },
    "autoprefix": {
      "__source": "./plugin/autoprefixer"
    },
    "html-autoprefix": {
      "__source": "./plugin/html-autoprefixer"
    },
    "tar-send": {
      "__source": "./plugin/tar-send"
    },
    "mini": {
      "__stop": false,
      "__source": "./plugin/mini",
      "__setting": {
        "ignore": [
          "(\\.min\\.js)$",
          "(\\.min\\.css)$"
        ],
        "js": {},
        "css": {},
        "html": {
          "js": true,
          "css": true
        }
      }
    },
    "hbs-ext": {
      "__source": "./plugin/hbs-ext"
    },
    "merge": {
      "__source": "./plugin/merge",
      "__setting": [
        {
          "_regexp": true,
          "target": "/js/global.js",
          "source": [
            "(\\-componts\\.js)$",
            "B-beMerge"
          ]
        },
        {
          "suffix": "js/",
          "postfix": ".js",
          "target": "global-1",
          "source": [
            "A-componts",
            "B-beMerge"
          ]
        }
      ]
    },
    "sp-proxy": {}
  },
  "silky": {
    "port": 14422
  },
  "silky-build": {
    "outdir": "./build",
    "ignore": [
      "node_modules",
      "(\\/\\.[^/]+)$",
      "plugin",
      "(css/module/)",
      "(\\.tar)$"
    ]
  },
  "__registry": "https://registry.npmjs.com/",
  "dependencies": {
    "sp-proxy": "^1.0.0"
  }
}
