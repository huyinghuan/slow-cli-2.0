{
  "name": "sample",
  "version": "1.0",
  "silky-version": "2.0.0",
  "silky-plugin": {
    "hbs": {
      "__stop": false,
      "__source": "./plugin/hbs",
      "__setting": {
        "root": "/template"
      }
    },
    "less": {
      "__stop": false,
      "__source": "./plugin/less",
      "__setting": {
        "_env_global": "global.less",
        "global": ["css/global.less"],
        "ignore": [
          "(css/module/)"
        ]
      }
    },
    "autoprefix": {
      "__stop": false,
      "__source": "./plugin/autoprefixer"
    },
    "html-autoprefix": {
      "__stop": false,
      "__source": "./plugin/html-autoprefixer"
    },
    "tar-send": {
      "__stop": false,
      "__source": "./plugin/tar-send"
    },
    "mini": {
      "__stop": false,
      "__source": "./plugin/mini",
      "__setting": {
        "ignore": [
          "(\\.min\\.js)$",
          "(\\.min\\.css)$"
        ],
        "js": {},
        "css": {},
        "html": {
          "js": true,
          "css": true
        }
      }
    },
    "hbs-ext": {
      "__stop": false,
      "__source": "./plugin/hbs-ext"
    },
    "sp-merge": [
      {
        "_regexp": true,
        "target": "/js/global.js",
        "source": [
          "(\\-componts\\.js)$",
          "B-beMerge"
        ]
      },
      {
        "suffix": "js/",
        "postfix": ".js",
        "target": "/js/global-1",
        "source": [
          "A-componts",
          "B-beMerge"
        ]
      }
    ],
    "sp-proxy": {}
  },
  "silky": {
    "port": 14422,
    "index": "index.html"
  },
  "silky-build": {
    "outdir": "./build",
    "ignore": [
      "node_modules",
      "(\\/\\.[^/]+)$",
      "plugin",
      "(\\.tar)$"
    ]
  },
  "__registry": "https://registry.npmjs.com/",
  "dependencies": {
    "sp-merge": "1.0.0",
    "sp-proxy": "1.0.0"
  }
}
