{
  "name": "sample",
  "version": "1.0",
  "silky-version": "2.0.0",
  "silky-plugin": {
    "hbs": {
      "__source": "./plugin/hbs",
      "__setting": {
        "root": "/template"
      }
    },
    "less": {
      "__source": "./plugin/less"
    },
    "autoprefix": {
      "__source": "./plugin/autoprefixer"
    },
    "html-autoprefix": {
      "__source": "./plugin/html-autoprefixer"
    },
    "tar-send": {
      "__source": "./plugin/tar-send"
    },
    "mini": {
      "__stop": false,
      "__source": "./plugin/mini",
      "__setting": {
        "ignore": [
          "(\\.min\\.js)$",
          "(\\.min\\.css)$"
        ],
        "js": {},
        "css": {},
        "html": {
          "js": true,
          "css": true
        }
      }
    },
    "hbs-ext": {
      "__source": "./plugin/hbs-ext"
    },
    "sp-proxy": [
      {
        "from": ["^/p", "^/t"],
        "to": "http://10.200.8.234:9810" 
      },{
        "from": ["^/api"],
        "to": "http://10.200.8.234:9810" 
      }
    ]
  },
  "silky": {
    "port": 14422
  },
  "silky-build": {
    "outdir": "./build",
    "ignore": [
      "node_modules",
      "(\\/\\.[^/]+)$",
      "plugin",
      "(css/module/)",
      "(\\.tar)$"
    ]
  },
  "__registry": "https://registry.npmjs.com/",
  "dependencies": {
    "sp-proxy": "^1.0.0"
  }
}
